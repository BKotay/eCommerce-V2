{
    "version": 2,
    "builds": [
      {
        "src": "server/package.json",
        "use": "@vercel/node",
        "config": {
            "includeFiles": ["server/**"]
        }
      },
      {
        "src": "src",
        "use": "@vercel/static-build",
        "config": {
            "distDir": "dist"
        }
      }
    ]
  }
  